{
  "name": "SuPlainText",
  "version": "1.0.0",
  "description": "Supercharged Plain Text Notes",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . SuPlainText --platform=win32 --arch=x64 --asar --overwrite --out=./out",
    "package-win32": "electron-packager . SuPlainText --platform=win32 --arch=x64 --asar --overwrite --out=./out",
    "package-linux": "electron-packager . SuPlainText --platform=linux --arch=x64 --asar --overwrite --out=./out"
  },
  "repository": "https://github.com/julius/suplaintext",
  "keywords": [
    "suplaintext",
    "plain",
    "text",
    "notes",
    "electron"
  ],
  "author": "Julius Eckert",
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-packager": "^9.0.0"
  },
  "dependencies": {}
}
